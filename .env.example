# Discord bot token
DISCORD_TOKEN=your_bot_token_here

# Comma-separated Discord user IDs allowed to run commands (required)
AUTH_USER_IDS=123456789012345678,987654321098765432

# Optional: comma-separated allowed command prefixes.
# If set, only commands that start with one of these prefixes will be executed.
# Examples: "ls,cat,top" will allow "ls -la", "cat file", "top" but not "rm -rf /"
COMMAND_WHITELIST=ls,cat,top,uptime

# Command prefix used in Discord messages
PREFIX=!term

# Execution settings (milliseconds and bytes)
EXEC_TIMEOUT_MS=10000
MAX_BUFFER_BYTES=200000
